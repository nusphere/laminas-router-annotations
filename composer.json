  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
    "symfony/routing": "*",
    "laminas/laminas-router": "*"
  },
  "require-dev": {
    "laminas/laminas-test": "*"
  },
  "autoload": {
    "psr-4": {
